{
    "Summary": "WAYMO Ablation CLFTv2-Tiny Fusion (Simple Average)",
    "tags": ["WAYMO", "Ablation", "CLFTv2-Tiny", "Simple Average"],
    "CLI": {"backbone": "swin_fusion", "mode": "cross_fusion", "path": "./waymo_dataset/splits_clft/"},
    "General": {
        "device": "cuda:0",
        "epochs": 50,
        "batch_size": 8,
        "path_predicted_images": "output",
        "seed": 0,
        "resume_training": false,
        "reset_lr": false,
        "early_stop_patience": 200,
        "max_epochs": 2,
        "model_path": ""
    },
    "Log": {"logdir": "logs/waymo/swin/ablation/ablation_2"},
    "SwinFusion": {
        "emb_dims": [96, 192, 384, 768],
        "hooks": [0, 1, 2, 3],
        "model_timm": "swinv2_tiny_window16_256",
        "fusion_strategy": "simple_average",
        "clft_lr": 8e-05,
        "warmup_epochs": 10,
        "patch_size": 4,
        "reassembles": [4, 8, 16, 32],
        "read": "ignore",
        "resample_dim": 256,
        "type": "segmentation",
        "lr_momentum": 0.99,
        "pretrained": true
    },
    "Dataset": {
        "name": "waymo",
        "dataset_root": "./waymo_dataset",
        "train_split": "./waymo_dataset/splits_clft/train_all.txt",
        "val_split": "./waymo_dataset/splits_clft/early_stop_valid.txt",
        "annotation_path": "annotation",
        "dataset_classes": [
            {"name": "ignore", "index": 0},
            {"name": "vehicle", "index": 1},
            {"name": "pedestrian", "index": 2},
            {"name": "sign", "index": 3},
            {"name": "cyclist", "index": 4},
            {"name": "background", "index": 5}
        ],
        "train_classes": [
            {"name": "background", "index": 0, "weight": 0.1, "dataset_mapping": [0, 5], "color": [0, 0, 0]},
            {"name": "vehicle", "index": 1, "weight": 5.0, "dataset_mapping": [1], "color": [128, 0, 128]},
            {"name": "sign", "index": 2, "weight": 35.0, "dataset_mapping": [3], "color": [255, 0, 0]},
            {"name": "human", "index": 3, "weight": 20.0, "dataset_mapping": [2, 4], "color": [0, 255, 255]}
        ],
        "transforms": {
            "resize": 256,
            "random_rotate_range": 20,
            "p_flip": 0.5,
            "p_crop": 0.3,
            "p_rot": 0.4,
            "image_mean": [0.485, 0.456, 0.406],
            "image_std": [0.229, 0.224, 0.225],
            "lidar_mean": [0.46137, 0.28761, 0.26726],
            "lidar_std": [0.11516, 0.12552, 0.09766]
        }
    }
}